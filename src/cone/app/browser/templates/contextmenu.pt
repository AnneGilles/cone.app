<tal:ajax xmlns:tal="http://xml.zope.org/namespaces/tal"
          xmlns:ajax="http://namesspaces.bluedynamics.eu/ajax"
          omit-tag="True">
  
  <div class="contextmenu">
    
    <tal:toolbars repeat="toolbar context.toolbars">
      <div tal:condition="toolbar.display"
           tal:content="structure toolbar(model, request)">
      </div>
    </tal:toolbars>
  
  </div>
  
  <div style="clear:both;"></div>
  
</tal:ajax>